{
  "name": "vue-mermaid-render",
  "type": "module",
  "version": "0.1.4",
  "packageManager": "pnpm@8.15.6",
  "license": "MIT",
  "repository": "hojas/vue-mermaid-render",
  "bugs": "https://github.com/hojas/vue-mermaid-render/issues",
  "keywords": [
    "vue",
    "vue-component",
    "component",
    "mermaid",
    "diagrams"
  ],
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/vue-mermaid-render.js",
      "require": "./dist/vue-mermaid-render.umd.cjs"
    }
  },
  "main": "./dist/vue-mermaid-render.umd.cjs",
  "module": "./dist/vue-mermaid-render.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "node ./scripts/dev.cjs",
    "dev:vue": "node ./scripts/dev.cjs --demo=vue",
    "dev:nuxt": "node ./scripts/dev.cjs --demo=nuxt",
    "build": "node ./scripts/build.cjs",
    "lint": "eslint .",
    "push": "pnpm build && npm publish",
    "prepare": "husky"
  },
  "dependencies": {
    "mermaid": "^10.9.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.3",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "shelljs": "^0.8.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.13"
  }
}
